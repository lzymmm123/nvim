{
    "markdownlint.config": {
        "line-length": false
    },
    "clangd.compilationDatabasePath": "build",
    "python.jediEnabled": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.position": "right",
    "explorer.width": 30,
    "explorer.previewAction.onHover": false,
    "explorer.icon.customIcons": {
        "icons": {
            "src":{
                "code": "",
                "color": "#ff0f0f"
            },
            "build":{
                "code": "",
                "color": "#ff0f0f"
            },
            "so": {
                "code": "",
                "color": "#8dc149"
            },
            "cmake":{
                "code":"煉",
                "color": "#00afaf"
            }
        },
        "extensions": {
            "so":"so",
            "cmake":"cmake"
        },
        "filenames": {
            "Makefile":"cmake",
            "CMakeLists.txt":"cmake"
        },
        "dirnames": {
            "src":"src",
            "build":"build"
        },
        "patternMatches": {},
        "dirPatternMatches": {}
    },
    "texlab.path": "texlab",
    "latex.build.executable":" latexmk",
    "latex.build.args": [
        "-pdf",
        "-pvc",
        "-xelatex",
        "-interaction=nonstopmode",
        "-synctex=1",
        "%f"
    ],
    "latex.forwardSearch.executable": "zathura",
    "latex.forwardSearch.args": [
        "--synctex-forward",
        "%l:1:%f",
        "%p"
    ],
    "snippets.ultisnips.directories": [
        "$HOME/.config/nvim/Ultisnips/",
        "$HOME/.config/nvim/plugged/vim-snippets/UltiSnips/"
    ],
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod"
            ],
            "disableWorkspaceFolders": true,
            "filetypes": [
                "go"
            ]
        }
    }
}
